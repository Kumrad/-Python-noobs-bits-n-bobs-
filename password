first_name = input('Enter first name: ')
second_name = input('Enter Surname: ')
full_name = f'{first_name} {second_name}'
message = f'welome {full_name.title()}.'
print(message) 
password = ''
while password != 'fiverslol' :
    password = input('please enter password: ')
if password == 'fiverslol' :
    print(full_name + ' You now have access!')
else:
    print('Try again!')
